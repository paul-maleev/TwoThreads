<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.twothreads.HelloController">
   <children>
      <Button fx:id="threadAButton" layoutX="26.0" layoutY="14.0" mnemonicParsing="false" onAction="#onThreadAButtonClick" text="Thread A" />
      <Button fx:id="threadBButton" layoutX="120.0" layoutY="14.0" mnemonicParsing="false" onAction="#onThreadBButtonClick" text="Thread B" />
      <TableView fx:id="outputTable" layoutX="20.0" layoutY="59.0" prefHeight="401.0" prefWidth="608.0">
        <columns>
          <TableColumn fx:id="ThreadColumn" prefWidth="75.0" text="Thread" />
          <TableColumn fx:id="MessageColumn" prefWidth="75.0" text="Message" />
            <TableColumn fx:id="TimeColumn" prefWidth="75.0" text="Time" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
